    "jshint": "2.9.1",
    "jshint-loader": "0.8.3",
    "less": "2.6.0",
    "less-loader": "2.2.2",
    "ng-annotate-loader": "0.1.0",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.0",
